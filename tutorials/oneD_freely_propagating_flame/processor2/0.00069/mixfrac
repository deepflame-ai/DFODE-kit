FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.00069";
    object      mixfrac;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform -1;

boundaryField
{
    boundary
    {
        type            empty;
    }
    inlet
    {
        type            calculated;
        value           uniform -1;
    }
    outlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform -1;
    }
}


